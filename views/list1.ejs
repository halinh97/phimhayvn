<!DOCTYPE html>
<html lang="en">
<head>
  <title>Bootstrap Example</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<link rel="stylesheet" href="../icon/css/font-awesome.min.css">
<link rel="stylesheet" href="../css/style1.css">
</head>
<body>

<div class="container">
  <div class="row">
    <div class="col-sm-8 col-md-push-2">
      <table class="table table-hover">
        <thead>
          <tr class="bg-success text-center">
            <td colspan="7"><a href="sigin"><strong>THÊM USERS</strong></a></td>
          </tr>
          <tr class="bg-primary">
            <th>id</th>
            <th>HỌ TÊN</th>
            <th>Username  </th>
            <th>password</th>
            <th>Edit </th>
            <th>Delete</th>
          </tr>
        </thead>

        <tbody>
        <% data.rows.forEach(function(item){ %>
            <tr>
              <td><%= item.id %></td>
              <td><%= item.localemail %></td>
              <td><%= item.mk %></td>
              <td><p><%= item.localpassword %></p></td>
              <td><a href="edit/<%= item.id %>" ><span class="fa fa-edit"> <span>Edit</span></span></a></td>
              <td> <a onclick="return confirm('ban co chac chan xoa?')"href="delete/<%= item.id %>"><span class="fa fa-trash-o"> <span>Delete</span></span></a></td>
            </tr>

          	<% });%>

        </tbody>
      </table>

    </div>
  </div>
</div>

</body>
</html>
